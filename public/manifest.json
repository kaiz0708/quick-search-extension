{
   "manifest_version": 3,
   "name": "Quick Search",
   "version": "1.0.0",
   "description": "Quick Search is a lightweight browser extension that helps you search the web faster with just one click.",
   "permissions": [],
   "background": {
      "service_worker": "background.js"
   },
   "action": {
      "default_popup": "index.html"
   },
   "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
   }
}
